{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"],
      "models/*": ["src/models/*"]
    }
  },
  "typeRoots": [
    "./src/@types"
  ],
  "lib": ["es2015"]
}
